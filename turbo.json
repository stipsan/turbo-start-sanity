{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "globalEnv": [
    "SANITY_API_READ_TOKEN",
    "SANITY_API_WRITE_TOKEN",
    "VERCEL_URL",
    "VERCEL_PROJECT_PRODUCTION_URL",
    "VERCEL_ENV",
    "NODE_ENV",
    "SANITY_STUDIO_VERCEL_GIT_REPO_ID",
    "SANITY_STUDIO_VERCEL_GIT_PROVIDER",
    "SANITY_STUDIO_VERCEL_GIT_REPO_OWNER",
    "SANITY_STUDIO_VERCEL_GIT_REPO_SLUG",
    "SANITY_STUDIO_VERCEL_PROJECT_PRODUCTION_URL",
    "SANITY_STUDIO_PROJECT_ID",
    "SANITY_STUDIO_DATASET",
    "SANITY_STUDIO_PRESENTATION_URL"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", ".sanity/**"]
    },
    "transit": {
      "dependsOn": ["^transit"]
    },
    "lint": {
      "dependsOn": ["transit"]
    },
    "format": {
      "dependsOn": ["transit"]
    },
    "format:check": {
      "dependsOn": ["transit"]
    },
    "check-types": {
      "dependsOn": ["transit"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
